{
    "_version_": "0.6dev.9",
    "cue": {
        "fadeActionDuration": 3,
        "fadeActionType": "Linear",
        "interruptFade": 3,
        "interruptFadeType": "Linear"
    },
    "theme": {
        "theme": "Dark",
        "icons": "numix"
    },
    "logging": {
        "limit": 10000,
        "msg_format": "%(asctime)s.%(msecs)03d\t%(name)s\t%(levelname)s\t%(message)s",
        "date_format": "%H:%M:%S",
        "backgrounds": {
            "DEBUG": [230, 230, 230, 200],
            "INFO": [80, 255, 65, 200],
            "WARNING": [255, 200, 0, 200],
            "ERROR": [255, 65, 0, 200],
            "CRITICAL": [255, 0, 0, 200]
        },
        "foregrounds": {
            "DEBUG": [0, 0, 0],
            "INFO": [0, 0, 0],
            "WARNING": [0, 0, 0],
            "ERROR": [255, 255, 255],
            "CRITICAL": [255, 255, 255]
        },
        "viewer": {
            "visibleLevels": ["WARNING", "ERROR", "CRITICAL"],
            "visibleColumns": ["asctime", "name", "message"]
        }
    },
    "backend": {
        "default": "gst"
    },
    "layout": {
        "default": "NoDefault",
        "stopAllFade": false,
        "pauseAllFade": false,
        "resumeAllFade": false,
        "interruptAllFade": true
    },
    "actions": {
        "maxStackSize": 0
    },
    "cartLayout": {
        "gridColumns": 7,
        "gridRows": 4,
        "showSeek": false,
        "showDbMeters": false,
        "showAccurate": false,
        "showVolume": false,
        "countdown": true,
        "autoAddPage": true
    },
    "listLayout": {
        "showDbMeters": true,
        "showSeek": true,
        "showAccurate": false,
        "showPlaying": true,
        "autoContinue": true,
        "endList": "Stop",
        "goKey": "Space",
        "stopCueFade": true,
        "pauseCueFade": true,
        "resumeCueFade": true,
        "interruptCueFade": true
    }
}
